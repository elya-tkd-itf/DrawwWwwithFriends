<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qaz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.DrawActivity"></activity>
        <activity android:name=".Activity.AddNew.AddNewDeskActivity" />
        <activity android:name=".Activity.AddNew.AddNewUserActivity" />
        <activity android:name=".Activity.GroupInfoActivity" />
        <activity android:name=".Activity.GroupMenuActivity" />
        <activity android:name=".Activity.AddNew.AddNewGroupActivity" />
        <activity
            android:name=".Activity.MyGroupsActivity"
            android:label="@string/title_activity_my_groups"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.LogInMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>